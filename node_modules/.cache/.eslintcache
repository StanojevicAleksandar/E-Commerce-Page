 }\n\n  function removeItem(item) {\n    setCart(cart.filter(book => book.id !== item.id))\n  }\n\n  function numberOfItems () {\n    let counter = 0;\n    cart.forEach(item => {\n      counter += item.quantity\n    })\n    return counter;\n  }\n\n  return (\n    <Router>\n      <div className=\"App\">\n        <Nav numberOfItems={numberOfItems()} />\n        <Routes>\n          <Route path=\"/\" element={<Home />} />\n          <Route path=\"/books\" exact element={<Books books={books} />} />\n          <Route path=\"/books/:id\" element={<BookInfo books={books} addToCart={addToCart} cart={cart} />} />\n          <Route path=\"/cart\" element={<Cart books={books} cart={cart} changeQuantity={changeQuantity} removeItem={removeItem} />} />\n        </Routes>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\Nav.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\Landing.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\Highlights.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\ui\\Highlight.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\Featured.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\ui\\book.jsx",["112"],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\data.js",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\Discounted.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\ui\\Book.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\Explore.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\Footer.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\pages\\home.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\pages\\Home.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\pages\\Books.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\pages\\BookInfo.jsx",["113"],[],"import React from \"react\";\r\nimport { Link, useParams } from \"react-router-dom\";\r\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\r\nimport Rating from \"../components/ui/Rating\";\r\nimport Price from \"../components/ui/Price\";\r\nimport Book from \"../components/ui/Book\";\r\nimport Cart from \"./Cart\";\r\n\r\n\r\nconst BookInfo = ( {books, addToCart, cart}) => {\r\n  const { id } = useParams();\r\n  const book = books.find(book => +book.id === +id);\r\n  \r\n\r\n  function addBookToCart(book) {\r\n    addToCart(book)\r\n  }\r\n\r\n  function bookExistsOnCart() {\r\n    return cart.find((book) => book.id === +id);\r\n  }\r\n\r\n  \r\n  return (\r\n    <div id=\"books__body\">\r\n      <main id=\"books__main\">\r\n        <div className=\"books__container\">\r\n          <div className=\"row\">\r\n            <div className=\"book__selected--top\">\r\n              <Link to=\"/books\" className=\"book__link\">\r\n                <FontAwesomeIcon icon=\"arrow-left\" />\r\n              </Link>\r\n              <Link to=\"/books\" className=\"book__link\">\r\n                <h2 className=\"book__selected--title--top\">Books</h2>{\" \"}\r\n              </Link>\r\n            </div>\r\n            <div className=\"book__selected\">\r\n              <figure className=\"book__selected--figure\">\r\n                <img src={book.url} alt=\"\" className=\"book__selected--img\" />\r\n              </figure>\r\n              <div className=\"book__selected--description\">\r\n                <h2 className=\"book__selected--title\">{book.title}</h2>\r\n                <Rating rating={book.rating} />\r\n                <div className=\"book__selected--price\">\r\n                  <Price\r\n                    originalPrice={book.originalPrice}\r\n                    salePrice={book.salePrice}\r\n                  />\r\n                </div>\r\n                <div className=\"book__summary\">\r\n                  <h3 className=\"book__summary--title\">Summary</h3>\r\n                  <p className=\"book__summary--para\">\r\n                    Lorem ipsum dolor sit amet consectetur, adipisicing elit.\r\n                    Mollitia, perferendis recusandae, ipsa qui repellendus amet\r\n                    quia assumenda sit quasi praesentium vitae sed, voluptatum\r\n                    delectus fugit harum? Magnam quis error ullam.\r\n                  </p>\r\n                  <p className=\"book__summary--para\">\r\n                    Lorem ipsum dolor sit amet consectetur, adipisicing elit.\r\n                    Mollitia, perferendis recusandae, ipsa qui repellendus amet\r\n                    quia assumenda sit quasi praesentium vitae sed, voluptatum\r\n                    delectus fugit harum? Magnam quis error ullam.\r\n                  </p>\r\n                </div>\r\n                { bookExistsOnCart() ? ( \r\n                <Link to={`/cart`} className=\"book__link\">\r\n                    <button className=\"btn\">Checkout</button>\r\n                </Link>\r\n                ) : (\r\n                    <button className=\"btn\" onClick={() => addBookToCart(book)}>Add to cart</button>\r\n                )\r\n                }\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"books__container\">\r\n          <div className=\"row\">\r\n            <div className=\"book__selected--top\">\r\n              <h2 className=\"book__selected--title--top\">Recommended Books</h2>\r\n            </div>\r\n            <div className=\"books\">\r\n              {books\r\n                .filter((book) => book.rating === 5 && +book.id !== +id)\r\n                .slice(0, 4)\r\n                .map((book) => (\r\n                  <Book book={book} key={book.id} />\r\n                ))}\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default BookInfo;\r\n","C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\ui\\Rating.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\components\\ui\\Price.jsx",[],[],"C:\\Users\\astan\\Desktop\\library-react-tutorial\\src\\pages\\Cart.jsx",["114","115"],[],"import React, { useState, useEffect } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport EmptyCart from \"../assets/empty_cart.svg\"\r\n\r\n\r\n\r\nconst Cart = ({ cart, changeQuantity, removeItem }) => {\r\n  const total = () => {\r\n    let price = 0;\r\n    cart.forEach((item) => {\r\n      price += +(item.salePrice || item.originalPrice * item.quantity\r\n        ).toFixed(2);\r\n    });\r\n    return price;\r\n  };\r\n  return (\r\n    <div id=\"books__body\">\r\n      <div id=\"books__main\">\r\n        <div className=\"books__container\">\r\n          <div className=\"row\">\r\n            <div className=\"book__selected--top\">\r\n              <h2 className=\"cart__title\">Cart</h2>\r\n            </div>\r\n            <div className=\"cart\">\r\n              <div className=\"cart__header\">\r\n                <span className=\"cart__book\">Book</span>\r\n                <span className=\"cart__quantity\">Quantity</span>\r\n                <span className=\"cart__total\">Price</span>\r\n              </div>\r\n              <div className=\"cart__body\">\r\n                {cart.map((book) => {\r\n                  return (\r\n                    <div className=\"cart__item\">\r\n                      <div className=\"cart__book\">\r\n                        <img\r\n                          src={book.url}\r\n                          alt=\"\"\r\n                          className=\"cart__book--img\"\r\n                        />\r\n                        <div className=\"cart__book--info\">\r\n                          <span className=\"cart__book--title\">\r\n                            {book.title}\r\n                          </span>\r\n                          <span className=\"cart__book--price\">\r\n                            CHF\r\n                            {(book.salePrice || book.originalPrice).toFixed(2)}\r\n                          </span>\r\n                          <button className=\"cart__book--remove\" onClick={() =>removeItem(book)}>Remove</button>\r\n                        </div>\r\n                      </div>\r\n                      <div className=\"cart__quantity\">\r\n                        <input\r\n                          type=\"number\"\r\n                          min={0}\r\n                          max={99}\r\n                          className=\"cart__input\"\r\n                          value={book.quantity}\r\n                          onChange={(event) =>\r\n                            changeQuantity(book, event.target.value)\r\n                          }\r\n                        />\r\n                      </div>\r\n                      <div className=\"cart__total\">\r\n                        CHF\r\n                        {(\r\n                          (book.salePrice || book.originalPrice) * book.quantity\r\n                        ).toFixed(2)}{\" \"}\r\n                      </div>\r\n                    </div>\r\n                  );\r\n                })}\r\n              </div>\r\n              {\r\n                cart.length === 0 &&\r\n              <div className=\"cart__empty\">\r\n                <img src={EmptyCart} alt=\"\" className=\"cart__empty--img\" />\r\n                <h2>You don't have any books in your cart!</h2>\r\n                <Link to=\"/books\">\r\n                <button className=\"btn\">Browse books</button>\r\n                </Link>\r\n              </div>\r\n              }\r\n            </div>\r\n           { cart.length && <div className=\"total\">\r\n              <div className=\"total__item total__sub-total\">\r\n                <span>Subtotal</span>\r\n                <span>CHF{(total() * 0.923).toFixed(2)}</span>\r\n              </div>\r\n              <div className=\"total__item total__tax\">\r\n                <span>Tax</span>\r\n                <span>CHF{(total() * 0.077).toFixed(2)}</span>\r\n              </div>\r\n              <div className=\"total__item total__price\">\r\n                <span>Total</span>\r\n                <span>CHF{total().toFixed(2)}</span>\r\n              </div>\r\n              <button\r\n                className=\"btn btn__checkout no-cursor\"\r\n                onClick={() => alert(`Haven't got around to doing this :(`)}\r\n              >\r\n                Proceed to checkout\r\n              </button>\r\n            </div>} \r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Cart;\r\n",{"ruleId":"116","severity":1,"message":"117","line":3,"column":8,"nodeType":"118","messageId":"119","endLine":3,"endColumn":14},{"ruleId":"120","severity":1,"message":"121","line":7,"column":7,"nodeType":"122","endLine":7,"endColumn":18},{"ruleId":"116","severity":1,"message":"123","line":7,"column":8,"nodeType":"118","messageId":"119","endLine":7,"endColumn":12},{"ruleId":"116","severity":1,"message":"124","line":1,"column":17,"nodeType":"118","messageId":"119","endLine":1,"endColumn":25},{"ruleId":"116","severity":1,"message":"125","line":1,"column":27,"nodeType":"118","messageId":"119","endLine":1,"endColumn":36},"no-unused-vars","'Footer' is defined but never used.","Identifier","unusedVar","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/jsx-eslint/eslint-plugin-jsx-a11y/blob/HEAD/docs/rules/anchor-is-valid.md","JSXOpeningElement","'Cart' is